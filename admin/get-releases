#!/bin/bash

if [ -n "$SF_LOGIN" ]; then
    LN=$SF_LOGIN
else
    LN=$LOGNAME
fi

TMP=/tmp/releases

ssh $LN,afp@shell.sourceforge.net "ls /home/project-web/afp/htdocs/release" > $TMP
cat $TMP | grep -v "current\\.tar\\.gz\\|devel\\.tar\\.gz\\|entry-status\\.txt\\|afp-....-..-..\\.tar\\.gz\\|afp-Example-Submission-....-..-..\\.tar\\.gz"
