#!/usr/bin/env bash
#
# $Id: devel-copy,v 1.5 2008-09-22 09:36:32 lsf37 Exp $
# Author: Gerwin Klein, NICTA
#
# Cron job that copies development snapshot to SF.
# Tested on sunbroy2 only.
# 

SOURCE=~isatest/html-data/afp/
TARGET=lsf37,afp@web.sourceforge.net:htdocs/

/usr/local/bin/rsync -rplqz $SOURCE $TARGET
