
## targets

default: BinarySearchTree
images: 
test: BinarySearchTree

all: images test


## global settings

SRC = $(ISABELLE_HOME)/src
OUT = $(ISABELLE_OUTPUT)
LOG = $(OUT)/log

USEDIR = $(ISATOOL) usedir -v true -i true -d pdf  ## -D generated


## BinarySearchTree

BinarySearchTree: $(LOG)/HOL-BinarySearchTree.gz

$(LOG)/HOL-BinarySearchTree.gz: ## BinarySearchTree/ROOT.ML BinarySearchTree/document/root.tex BinarySearchTree/*.thy
	@$(USEDIR) HOL BinarySearchTree


## clean

clean:
	@rm -f $(LOG)/HOL-BinarySearchTree.gz
