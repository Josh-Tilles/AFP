
## targets

default: KBPs
images: KBPs
test: 

all: images test

## global settings

SRC = $(ISABELLE_HOME)/src
OUT = $(ISABELLE_OUTPUT)
LOG = $(OUT)/log

USEDIR = $(ISABELLE_TOOL) usedir -v true -i true -d pdf -D generated


## KBPs

KBPs: $(OUT)/KBPs document/root.*

$(OUT)/KBPs: ROOT.ML document/root.tex document/root.bib *.thy
	@$(USEDIR) -b HOL KBPs


## clean

clean:
	@rm -f $(OUT)/KBPs
